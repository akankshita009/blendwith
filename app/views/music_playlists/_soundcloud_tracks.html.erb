<% data = { type: 'track', id: item_id, network: 'soundcloud', title: track.title, tags: track.tags.map(&:name).join(","), url: track_path(track) } %>
<%= content_tag :li, data: data, class: 'span3 with-title audio-li thumbnail' do %>
  <!-- <input id="img_<%#= track.track_object[:id] %>" type="checkbox" value="<%#= track.track_object[:id] %>"> -->
  <%= track.title %>
  <% # link_to "Preview", soundcloud_embed_ajax_index_path({ track_url: track.secret_uri, track_id: track.id}), remote: true, data: { track_id: track.id }, :class => "preview" %>
  <%= link_to "Play", "#", class: 'play', data: {track_id: track.social_network_id} %>
  <%= content_tag :div, "", :id => "track-#{track.social_network_id}", :class => "track-preview" %>
  <div class="player">
    <span class="play-button"></span>
    <span class="progress-bar"></span>
  </div>

    <% if action_name == 'show' %>
        <!--<span class="awe-cog"></span>-->
        <a href="#demoModal" data-toggle="modal" class="awe-trash"></a>
    <% end %>
<% end %>

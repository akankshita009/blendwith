<script src="http://vjs.zencdn.net/4.1/video.js"></script>
<%= javascript_include_tag "plugins/player_cheage/vjs.youtube.js" %>
<style>
    .vjs-default-skin .vjs-big-play-button {
        display: none;
    }

    .vjs-control-bar {
        position: absolute;
        bottom: 0;
    }
</style>

<!-- Main data container -->
<div>
  <div class="row-fluid">
    <%= render "video_networks/left_nav" %>

    <article class="span10 special">
      <div class="content">
        <div class="row-fluid">
          <div class="center">
            <!-- Secondary navigation -->
          </div>
          <div class="span10 center">
            <div class="nav-secondary">
              <%= render 'video_networks/social_nav' %>
            </div>
            <!-- /Secondary navigation -->
          </div>
        </div>
        <div class="row-fluid">
          <div class="page-header custom">
            <h3>URL <span class="awe-cog"></span><br/>
              <small>Select all | Select none</small>
            </h3>


          </div>
        </div>
        <%= render 'video_form' %>
        <div class="row-fluid">
          <div class="page-container">
            <form class="form-gallery">
              <ul id="thumbnails" class="thumbnails">
                <%= render 'videos' %>
              </ul>
            </form>
          </div>
        </div>

      </div>
      <div class="form">
        <a href="<%= image_path("sample_content/sample-image-900x500.png") %>" class="form-image">
          <img class="element" src="<%= image_path("sample_content/sample-image-250x150.png") %>" alt=""/>
          <span class="awe-zoom-in"></span>
        </a>

       
      <%= render :partial => 'video_edit' %>
 
      </div>
    </article>
  </div>
</div>

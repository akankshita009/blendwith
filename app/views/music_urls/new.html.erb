<link rel="stylesheet" href="http://code.jquery.com/ui/1.10.3/themes/smoothness/jquery-ui.css" type="text/css" media="screen" />
<!-- Main data container -->


<div>

  <div class="row-fluid">

    <%= render "music_networks/left_nav" %>

    <article class="span10 special">
      <div class="content">
        <div class="row-fluid">
          <div class="center">
            <!-- Secondary navigation -->
          </div>
          <div class="span10 center">
            <div class="nav-secondary">
              <%= render 'music_networks/social_nav' %>
            </div>
            <!-- /Secondary navigation -->
          </div>
        </div>
        <div class="row-fluid">
          <div class="page-header custom">
            <h3>A Great Playlist Title <span class="awe-cog"></span><br/>
              <small>Select all | Select none</small>
            </h3>


          </div>
        </div>

        <%= render 'music_form' %>
        <br />
        <br />
        <br />
        <%= render 'shared/audiojs_player' %>
        <br />
        <br />
        <br />

        <div class="clearfix"></div>

        <div class="row-fluid">
          <div class="page-container">
            <form class="form-gallery">
              <ul id="thumbnails">
            <li class="span12"></li>
                <%= render partial: "music_partial", :collection => @music_items, as: :item, locals: { playlist: @playlist, network: ''}  %>
              </ul>
            </form>
            <%= render 'application/edit_form' %>
          </div>
        </div>

      </div>

    </article>
  </div>
</div>

<%#= javascript_include_tag 'plugins/audio' %>
<%= render "application/audio_control" %>
